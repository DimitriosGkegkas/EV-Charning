<%- include("navBar.ejs")%>
    <%- include("searchSessionsForm.ejs")%>
    <% const list = body.SessionsSummaryList %>

<div class= " container justify-content-center "> 
        <h1 class= " container justify-content-center mb-4 float-left">Sessions per   <%= per %>  <div class="text-info float-right"><%= body.StationID %></div> 
        </h1>
    </div>
    <div style="height:30px"> </div>
    <div class="container">
      <div id="chart"></div>
      <div style="height:30px"> </div>
      <div id="chartCount"></div>
    </div>
    <div style="height:30px"> </div>
<div class="container justify-content-center md-5  border-line">
        <table class="table table-striped table-hover table-dark">
            <thead >
              <tr>
                <th scope="col">Point ID</th>
                <th scope="col">Point Sessions</th>
                <th scope="col">Energy Delivered</th>
              </tr>
            </thead>
            <tbody>
            <% for ( i of list ) { %>
                <tr>
                <th scope="row"><%= i._id %></th>
                <td><%= i.PointSessions %></td>
                <td><%= i.EnergyDelivered %></td>

              </tr>
                
            <% }%>

            </tbody>
          </table>

        </div>
     
