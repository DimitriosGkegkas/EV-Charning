<%- include("navBar.ejs")%>
    <%- include("searchSessionsForm.ejs")%>
    <% const list = body %>

    <div class= " container justify-content-center "> 
      <h1 class= "border-bottom border-primary p-3 container justify-content-center mb-4 float-left">Sessions per   <%= per %>  <div class="text-info float-right"><%= body.ProviderID %></div> 
      </h1>
  </div>


<div class="container justify-content-center md-5  border-line">
        <table class="table table-striped table-hover table-dark">
            <thead >
              <tr>
                <th scope="col">Provider Name</th>
                <th scope="col">Station ID</th>
                <th scope="col">Session ID</th>
                <th scope="col">Vehicle ID</th>
                <th scope="col">Started On</th>
                <th scope="col">Finished On</th>
                <th scope="col">Energy Delivered</th>
                <th scope="col">Price Policy</th>
                <th scope="col">Cost per kWh</th>
                <th scope="col">Total Cost</th>
              </tr>
            </thead>
            <tbody>
            <% for ( i of list ) { %>
                <tr>
                <th scope="row"><%= i.ProviderID %></th>
                <td><%= i.StationID %></td>
                <td><%= i.SessionID %></td>
                <td><%= i.VehicleID %></td>
                <td><%= i.StartedOn %></td>
                <td><%= i.FinishedOn %></td>
                <td><%= i.EnergyDelivered %></td>
                <td><%= i.PricePolicyRef %></td>
                <td><%= i.CostPerKWh %></td>
                <td><%= i.TotalCost %></td>
              </tr>
                
            <% }%>

            </tbody>
          </table>

        </div>
     
        <%- include("footer.ejs")%>